<% include ../_partial/_header %>
<div class="header-wrapper">
  <header class="header">
    <div class="header-image-layout">
      <div class="header-image-wrap">
        <video
          class="header-image header-image-type-video js-header-video"
          src=""
          alt=""
          autoplay
          playsinline
          muted
          loop
          width="500"
        ></video>
        <img
          class="header-image header-image-type-image js-header-image"
          src=""
          alt=""
        />
      </div>
    </div>
    <div class="header-content">
      <h1 class="header-title js-title">Header Title</h1>
      <div class="header-subtitle-wrap js-header-subtitle-wrap">
        <p class="header-subtitle js-header-subtitle">Header Subtitle</p>
      </div>
      <div class="countdown js-countdown">
        <div class="countdown-days">
          <div class="countdown-value js-countdown-days">000</div>
          <div class="countdown-label js-label-days">Day</div>
        </div>
        <div class="countdown-hours">
          <div class="countdown-value js-countdown-hours">00</div>
          <div class="countdown-label js-label-hours">Hr</div>
        </div>
        <div class="countdown-mins">
          <div class="countdown-value js-countdown-mins">00</div>
          <div class="countdown-label js-label-mins">Min</div>
        </div>
        <div class="countdown-secs">
          <div class="countdown-value js-countdown-secs">00</div>
          <div class="countdown-label js-label-secs">Sec</div>
        </div>
      </div>
      <div class="action-button">
        <a
          href=""
          class="button button-primary button-large js-action-button"
        >
          Action Button
        </a>
      </div>
    </div>
  </header>
</div>
<div class="section-callout">
  <div class="container">
    <div class="callout-wrap js-callout-wrap">
      <div class="callout js-callout"></div>
    </div>
  </div>
</div>
<section class="section-stream js-stream">
  <div class="stream-player-container">
    <%# Twitch %>
    <div class="stream-player" id="twitch-embed">
      <iframe
        class="stream-player-player js-stream-player"
        src="https://player.twitch.tv/?channel=example&parent=example.com"
        frameborder="0"
        allowfullscreen="true"
        scrolling="no"
        width="620"
        height="378"
        allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
      >
      </iframe>
    </div>
    <%# YouTube Live %>
    <div class="stream-player" id="youtube-embed">
      <iframe
        class="stream-player-player js-stream-player"
        src="https://www.youtube.com/embed/"
        frameborder="0"
        allowfullscreen="true"
        scrolling="no"
        width="620"
        height="378"
        allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
      >
      </iframe>
    </div>
  </div>
  <div class="stream-marquee stream-marquee-top">
    <span class="marquee-text">
      Livestream Livestream Livestream Livestream
    </span>
    <span class="marquee-text">
      Livestream Livestream Livestream Livestream
    </span>
  </div>
  <div class="stream-marquee">
    <span class="marquee-text">
      Livestream Livestream Livestream Livestream
    </span>
    <span class="marquee-text">
      Livestream Livestream Livestream Livestream
    </span>
  </div>
</section>
